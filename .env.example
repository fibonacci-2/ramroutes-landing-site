# Firebase Configuration for Client-Side React App
# Copy this file to .env.local and fill in your actual values

# Get these values from Firebase Console > Project Settings > General Tab > Your Apps
REACT_APP_FIREBASE_API_KEY=your-api-key-here
REACT_APP_FIREBASE_AUTH_DOMAIN=trials-of-venus.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=trials-of-venus
REACT_APP_FIREBASE_STORAGE_BUCKET=trials-of-venus.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-sender-id-here
REACT_APP_FIREBASE_APP_ID=your-app-id-here

# Optional: For development with Firebase Emulator
REACT_APP_USE_FIREBASE_EMULATOR=false
REACT_APP_FIRESTORE_EMULATOR_HOST=localhost
REACT_APP_FIRESTORE_EMULATOR_PORT=8080
